  "source": "./src/views/index/index.jsx",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "webpack": "webpack --mode=production",
    "watch-sass": "sass --watch ./public/css:./public/css",
    "start-web": "npm run clean && concurrently \"npm run watch-sass\" \"parcel ./src/views/index/index.jsx\"",
    "clean": "rm -rf build && rm -rf dist && rm -rf .parcel-cache",
    "build": "NODE_ENV=production npm run clean && parcel build --target frontend"
  },
  "targets": {
    "frontend": {
      "source": "./public/html/index.html",
      "distDir": "./build",
      "context": "browser",
      "includeNodeModules": true
    }
  },
